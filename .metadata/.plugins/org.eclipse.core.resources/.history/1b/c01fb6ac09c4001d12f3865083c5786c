package com.example.cruddemo.Entity;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;

@Entity
@Table(name="student")
public class Student {

	@Id
	@Gene
	@Column(name="id")
	private int id;
	
	
	
	@Column(name="first_name")
	private String name;
	
	
	@Column(name="last_name")
	
	
	
	
	
	
	
	
	
	public Student() {
		// TODO Auto-generated constructor stub
	}
	
	
	
	

}
